{
  "name": "flutter-fuse",
  "displayName": "Flutter Fuse",
  "description": "Better Import Management for Flutter",
  "version": "0.0.1",
  "publisher": "RukshanJS",
  "icon": "icons/flutter-fuse.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/RukshanJS/flutter-fuse.git"
  },
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Formatters",
    "Debuggers"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "flutter-fuse.quickFix",
        "title": "Flutter Fuse: Fix Imports"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.77.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5",
    "@vscode/test-electron": "^2.3.0"
  }
}